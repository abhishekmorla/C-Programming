#include <stdio.h>

void sort(int n , int number[20])
{
	int temp;
	for (int i = 0; i < n; ++i) 
	{
		for (int j = i + 1; j < n; ++j)
		{
			if (number[i] > number[j]) 
			{
				 temp =  number[i];
				  number[i] = number[j];
				 number[j] = temp;
            }
            }
        }
}

int main() 
{
	int t;
	int arr[20];
	printf("Number of test cases: \n");
	scanf("%d",&t);
	int tc=1;
	while(t--)
	{
		int asknum,budget;
		printf("Write total number of cost of houses:");
		scanf("%d",&asknum);
		printf("Write total budget:");
		scanf("%d",&budget);
		for(int i=0; i < asknum ; i++)
		{
			scanf("%d",&arr[i]);
		}
		// for sorting
		sort(asknum,arr);
		int ans=0;
		
		for(int i=0; i < asknum ; i++)
		{
			if (budget >= arr[i])
			{
				ans++;
				budget -= arr[i];
			}
		}
		printf("Case %d#: %d",tc,ans);
	}
}

